[Unit]
Description=Blackbox Exporter Service
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
ExecStart=/opt/blackbox_exporter/blackbox_exporter-0.19.0.linux-amd64/blackbox_exporter --config.file==/opt/blackbox_exporter/blackbox_exporter-0.19.0.linux-amd64/blackbox.yml

[Install]
WantedBy=multi-user.target