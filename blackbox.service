[Unit]
Description=Blackbox Exporter Service
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
User=ubuntu
Group=ubuntu
ExecStart=/home/ubuntu/blackbox_exporter-0.19.0.linux-amd64/blackbox_exporter --config.file==/home/ubuntu/blackbox_exporter-0.19.0.linux-amd64/blackbox.yml

[Install]
WantedBy=multi-user.target